{
  "name": "@dangreen/optimize-plugin-demo",
  "version": "1.3.0",
  "description": "",
  "repository": "git://github.com/dangreen/optimize-plugin-demo.git",
  "publishConfig": {
    "directory": "package"
  },
  "scripts": {
    "list": "ls -al dist",
    "build:base": "VARIANT=base webpack build --config ./webpack.config.js",
    "build:babel-modern": "VARIANT=babel-modern webpack build --config ./webpack.config.js",
    "build:babel-legacy": "VARIANT=babel-legacy webpack build --config ./webpack.config.js",
    "build:optimize-plugin": "VARIANT=optimize-plugin webpack build --config ./webpack.config.js",
    "build": "yarn build:base && yarn build:babel-modern && yarn build:babel-legacy && yarn build:optimize-plugin && yarn run list",
    "prepublishOnly": "rm -rf package && clean-publish",
    "postpublish": "rm -rf package"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "core-js": "^3.20.1",
    "p-limit": "^4.0.0",
    "preact": "^10.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@trigensoftware/optimize-plugin": "1.1.0",
    "babel-loader": "^8.2.3",
    "browserslist": "^4.18.1",
    "clean-publish": "^3.4.5",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.9.1"
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package",
    "files": ["src", "webpack.config.js"],
    "fields": ["publishConfig"]
  },
  "files": [
    "dist"
  ]
}
